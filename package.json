{
  "name": "n8n-nodes-resend",
  "version": "2.3.1",
  "description": "Resend integration for n8n with full API coverage: Email, Broadcasts, Templates, Domains, Contacts, Segments, Topics, Webhooks, and more",
  "keywords": [
    "n8n-community-node-package",
    "resend",
    "email",
    "webhook",
    "n8n",
    "automation",
    "integration",
    "broadcast",
    "template",
    "contacts"
  ],
  "license": "MIT",
  "homepage": "https://github.com/resend/n8n-nodes-resend",
  "author": {
    "name": "Resend",
    "email": "devsilkepilon@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/resend/n8n-nodes-resend.git"
  },
  "engines": {
    "node": ">=20.15"
  },
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && tsc && gulp build:icons && auto-changelog",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials",
    "lintfix": "eslint nodes credentials --fix",
    "prepublishOnly": "npm run build && npm run lint"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/ResendApi.credentials.js",
      "dist/credentials/ResendWebhookSigningSecretApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Resend/Resend.node.js",
      "dist/nodes/Resend/ResendTrigger.node.js"
    ]
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^25.2.0",
    "@typescript-eslint/parser": "~8.55.0",
    "eslint": "^9.39.2",
    "eslint-plugin-n8n-nodes-base": "^1.16.5",
    "gulp": "^5.0.0",
    "prettier": "^3.5.3",
    "rimraf": "^6.1.2",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
