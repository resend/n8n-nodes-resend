{
  "name": "n8n-nodes-resend",
  "version": "2.0.1",
  "description": "Resend integration for n8n with full API coverage: Email, Broadcasts, Templates, Domains, Contacts, Segments, Topics, Webhooks, and more",
  "keywords": [
    "n8n-community-node-package",
    "resend",
    "email",
    "webhook",
    "n8n",
    "automation",
    "integration",
    "broadcast",
    "template",
    "contacts"
  ],
  "license": "MIT",
  "homepage": "https://github.com/SilkePilon/n8n-nodes-resend/",
  "author": {
    "name": "SilkePilon",
    "email": "silkepilon@users.noreply.github.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SilkePilon/n8n-nodes-resend.git"
  },
  "engines": {
    "node": ">=20.15"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build && npm run lint"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/ResendApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Resend/Resend.node.js",
      "dist/nodes/Resend/ResendTrigger.node.js"
    ]
  },
  "dependencies": {
    "isbot": "^3.6.13"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.21",
    "@typescript-eslint/parser": "~8.54.0",
    "eslint": "^8.57.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "gulp": "^5.0.0",
    "n8n-workflow": "^2.6.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2"
  },
  "peerDependencies": {
    "n8n-workflow": ">=2.6.0"
  }
}
